<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="wrap_content"
    android:layout_height="wrap_content" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".BreadofLife.BreadOfLife"
    android:id="@+id/BFL"
    android:background="@drawable/mainbof">


    <TableLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:id="@+id/tableLayout4"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true">





        <TableRow
         >


            <ImageButton
                android:id="@+id/GoogleMaps"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginRight="30dp"

                android:background="@drawable/gps"
                android:onClick="onClick"
                android:layout_column="1" />

            <ImageButton
                android:id="@+id/feed"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginRight="30dp"
                android:onClick="onClick"
                android:background="@drawable/share"
                android:layout_column="2" />

            <ImageButton
                android:id="@+id/events"
                android:layout_width="90dp"
                android:layout_height="90dp"

                android:onClick="onClick"
                android:background="@drawable/eventsicon2"
                android:layout_column="3" />


            <!---   Any buttons Put here Keep my rows Like this Jacob -->
        </TableRow>


        <TableRow android:layout_marginTop="16dp"

            android:layout_column="5">


            <ImageButton
                android:id="@+id/donate"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginRight="30dp"
                android:onClick="onClick"
                android:clickable="true"
                android:background="@drawable/paypal_button"
                android:layout_column="1" />

            <ImageButton
                android:id="@+id/info"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/info_button"
                android:onClick="onClick"
                android:layout_column="2" />


            <ImageButton
                android:id="@+id/test"
                android:layout_width="90dp"
                android:layout_height="90dp"

                android:onClick="onClick"
                android:background="@drawable/icon_add"
                android:layout_column="3" />


            <!---   text font Copper gothic bold size 40 -->
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"></TableRow>

    </TableLayout>



    <ImageButton
        android:id="@+id/frown"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="30dp"
        android:onClick="onClick"
        android:background="@drawable/frown"
        android:layout_alignTop="@+id/smile"
        android:layout_toEndOf="@+id/smile" />

    <ImageButton
        android:id="@+id/smile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:onClick="onClick"
        android:background="@drawable/smiley"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>


